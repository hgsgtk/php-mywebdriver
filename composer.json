{
    "name": "hgsgtk/phpwd",
    "description": "Webdriver client for PHP",
    "keywords": [
        "webdriver",
        "selenium",
        "php",
        "chromedriver"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hgsgtk",
            "email": "hgsgtk@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Phpwd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpwd\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "tests": "phpunit tests"
    }
}
